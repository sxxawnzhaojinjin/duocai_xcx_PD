  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.box {
  width: 85%;
  height: auto;
  background-color: #fff;
  z-index: 9500;
  border-radius: 8rpx;
}

.startPrint {
  margin-top: 100rpx;
  width: 60% !important;
  background-color: #1A50B2;
  color: #fff;
  font-size: 28rpx;
  border-radius: 10rpx;
}

.print_model_title {
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
}

.print_model_box {
  display: flex;
  width: 100%;
  padding: 20rpx 0;
}

.print_model_image {
  /* flex: 1; */
  display: flex;
  justify-content: center;
  align-items: center;
  width: 25%;
}

.print_model_info {
  /* flex: 2; */
  width: 75%;
}

.print_model_info_name {
  font-size: 32rpx !important;
  color: #1A50B2 !important;
  width: 98%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.print_model_info view {
  font-size: 24rpx;
  color: #999999;
}

.print_model_size {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
  font-size: 24rpx;
  color: #999999;
}

.print_model_img {
  width: 120rpx;
  height: 120rpx;
  background-color: #D8D8D8;
  border-radius: 8rpx;
}

.print_model_num {
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
}

.print_num_title {
  /* flex: 1; */
  flex: 0 0 200rpx;
  text-align: right;
}

.input {
  flex: 1;
  height: 60rpx;
  line-height: 60rpx;
  color: #999999;
  border: 2rpx solid #CCCCCC;
  padding: 0 10rpx;
}

.print_num_fen {
  flex: 1;
}

.print_model_footer {
  margin-top: 40rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  border-top: 2rpx solid #F1F3F5;
}

.print_model_cancel {
  text-align: center;
  flex: 1;
  background-color: #ffffff;
  color: #225FD9;
  border-bottom-left-radius: 8rpx;
}

.print_model_start {
  flex: 1;
  background-color: #1A50B2;
  text-align: center;
  color: #ffffff;
  border-bottom-right-radius: 8rpx;
}


/** 滑动 **/
.cart {
  width: 100%;
  box-sizing: border-box;
  /* height: 100%;
  overflow-y: scroll;
  overflow-x: hidden; */
}

.content {
  padding-bottom: 150rpx;
  /* margin-top: 15rpx */
}

.item_list {
  display: flex;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 2rpx solid #F1F3F5;
  height: 150rpx;
}

/* 除删除按钮之外的其他部分 */
.item-left {
  display: flex;
  justify-content: start;
  align-items: center;

  /* 关键代码 start */
  width: 100%;
  margin-left: -300rpx;
  transform: translateX(300rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(300rpx);
  /* 关键代码 end */
}

.item-left .icon-check {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.icon-check image {
  width: 45rpx;
  height: 45rpx;
  display: block;
}

.imgs {
  width: 200rpx;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.imgs image {
  width: 200rpx;
  height: 200rpx;
  display: block;
}

.context {
  padding-left: 20rpx;
}

.context .title {
  color: #333;
  font-size: 34rpx;
}

.context .bottoms {
  display: flex;
  padding-top: 20rpx;
}

.bottoms .price {
  width: 36rpx;
  color: #FD5761;
  padding-right: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottoms .num {
  display: flex;
  justify-content: start;
  align-items: center;
}

.bottoms .num .opare {
  padding: 10rpx 24rpx;
  border: 1rpx solid #ccc;
}

.num .nums {
  margin: 0 10rpx;
}

/* 删除按钮 */

.delete {
  width: 300rpx;
  display: flex;
  font-size: 28rpx;
  color: #fff;
  /* 关键代码 start */
  transform: translateX(310rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(310rpx);
  /* 关键代码 end */
  font-size: 26rpx;
  text-align: center;
}

.delete_text {
  display: flex;
  align-items: center;
  justify-content: center;
}

.text_see {
  flex: 3;
  background-color: #1A50B2;
}

.text_delete {
  flex: 2;
  background-color: #F95151;
}


/* 关键代码 start --> 向左滑动 */
.touch-move-active .item-left,
.touch-move-active .delete {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}


.customer-list {
  justify-content: space-between;
  font-size: 28rpx;
}

.customer-left {
  display: flex;
  flex-direction: column;
}

.customer-left .gender {
  font-size: 24rpx;
  color: #666;
  max-width: 300rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.customer-right {
  display: flex;
  align-items: center;
  justify-content: center;
}

.next {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}



/**  底部 **/
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
}

.footer_empty {
  text-align: center;
  flex: 1;
  background-color: #ffffff;
  color: #225FD9;
}


.footer_add {
  flex: 1;
  background-color: #1A50B2;
  text-align: center;
  color: #ffffff;
}


