page {
  width: 100%;
  height: 100%;
  background-color: #F1F3F5;
}

.print_container {
  width: 100%;
  box-sizing: border-box;	
  overflow-x: hidden;
}
.height100{
  height: 100%;
}

.noData_box {
  width: 100%;
  height: 100%;
  position: relative;
}

.noData_info {
  width: 100%;
  text-align: center;
  position: absolute;
  top: 40%;
  /*偏移*/
  transform: translateY(-50%);
}

.noData_info view:nth-child(1) {
  height: 200rpx;
}

.noData_info view:nth-child(2) {
  margin: 40rpx 0;
  font-size: 32rpx;
}

.noData_img {
  width: 200rpx;
  height: 200rpx;
}

.noData_text {
  color: #999999;
  font-size: 28rpx;
}

.addGoods {
  width: 90% !important;
  background-color: #1A50B2;
  color: #fff;
  font-size: 32rpx;
  border-radius: 10rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 auto;
}


/** 列表 **/
.bg {
  display: flex;
  flex-direction: column;
  background-color: #f3f3f3;
  width: 100%;
}

.list_bg {
  flex-direction: column;
  display: flex;
  background-color: #f3f3f3;
}

.item {
  background-color: white;
  border-radius: 10rpx;
  display: flex;
  justify-content: space-between;
  flex: 1;
  height: 120rpx;
}

.image {
  /* display: flex; */
  /* margin-top: 10rpx; */
  margin-left: 10rpx;
  height: 120rpx;
  line-height: 120rpx;
  width: 120rpx;
  border-radius: 10rpx;
  background-color: #D8D8D8;
  text-align: center;
}

.title_bg {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.left_bg {
  display: flex;
  justify-content: flex-start;
  background-color: #fff;
  border-top: 2rpx solid #F1F3F5;
}

.title {
  color: #1A50B2;
  font-size: 32rpx;
  margin-left: 20rpx;
  width: 420rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;

}

.lab {
  color: #999;
  font-size: 24rpx;
}


.title_r {
  font-size: 24rpx;
  color: #999999;
  display: flex;
  flex-direction: column;
  padding: 10rpx 0rpx;
  justify-content: space-between;
  margin-right: 20rpx;
  text-align: right;
}

.next {
  height: 30rpx;
  width: 30rpx;
  margin-right: 20rpx;
}

.title_r_text {
  margin-right: 40rpx;
  font-size: 28rpx
}


/** 模态框 **/
.noscoll{
  height: 100vh;
  overflow: hidden;
}
